<view class="page" >

  <view class='list'>
    <view class="com_title">{{date}}</view>
    <view class='com_money'>￥{{add}}元</view>
  </view>
  <!--url="/pages/complete/details/details?title=details"   -->
  <navigator url="/pages/complete/details/details?title=details" bindtap="listClick" class="weui-cell weui-cell_access" id="{{index}}" wx:for="{{zhihu}}" >
    <view class='order'>
      <view class='lists'>
        <view class='lists_l'>
          <view>订单编号</view>
          <view>{{item.id}}</view>
        </view>
        <view class='lists_r'>
          <text class='red'> +{{item.type}}</text>元</view>
      </view>
      <view class='paytime'>付款时间{{item.ga_prefix}}</view>
    </view>
  </navigator>
</view>