
<!--预约和取消的订单详情  -->
<template name="orderDetail">
<view class="page">
   <view class='det_title'>
    <view class='title_n'>{{name}}</view>
    <view class='title_m'>+{{get}}</view>
  </view>
 <view class="weui-cells weui-cells_after-title">
    <view class="weui-cell">
      <view class="weui-cell__bd">商品名称</view>
      <view class="weui-cell__ft">{{name}}￥{{price}}x{{page}}</view>
    </view>
     <view class="weui-cell">
      <view class="weui-cell__bd">预约时间</view>
      <view class="weui-cell__ft">{{time}}</view>
    </view>
  </view>

<view class="weui-cells weui-cells_after-title">
     <view class="weui-cell">
      <view class="weui-cell__bd">联系人</view>
      <view class="weui-cell__ft">{{name}}</view>
    </view>
     <view class="weui-cell">
      <view class="weui-cell__bd">联系电话</view>
      <view class="weui-cell__ft">{{number}}</view>
    </view>
  </view>

  <view class="weui-cells weui-cells_after-title">
     <view class="weui-cell">
      <view class="weui-cell__bd">订单编号</view>
      <view class="weui-cell__ft">{{number}}</view>
    </view>
     <view class="weui-cell">
      <view class="weui-cell__bd">订单时间</view>
      <view class="weui-cell__ft">{{time}}</view>
    </view>
  </view>

</view>
</template>